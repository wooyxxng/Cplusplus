// 시험 성적

#include <iostream>
using namespace std;

int main()
{
	int exam_score;

	cin >> exam_score;

	if (90 <= exam_score && exam_score <= 100)
		cout << "A" << endl;
	else if (80 <= exam_score && exam_score <= 89)
		cout << "B" << endl;
	else if (70 <= exam_score && exam_score <= 79)
		cout << "C" << endl;
	else if (60 <= exam_score && exam_score <= 69)
		cout << "D" << endl;
	else
		cout << "F" << endl;
	return 0;
}
