// 곱셉

#include <iostream>
#include <string>
using namespace std;
int main()
{
	int i, j, k, l, m, n, o;

	cin >> i;
	cin >> j;

	l = i * (j % 10);
	m = i * (j / 10 % 10);
	n = i * (j / 100);
	o = i * j;

	cout << to_string(l);
	cout << "\n";
	cout << to_string(m);
	cout << "\n";
	cout << to_string(n);
	cout << "\n";
	cout << to_string(o);
	return 0;
}
