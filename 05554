//5554번 심부름 가는 길
//초(시간)를 4개 입력받아 분초로 바꾸어 출력하기
//input : s1,s2,s3,s4
//output : m,s

#include <iostream>
using namespace std;

int main()
{
	int s1, s2, s3, s4;

	cin >> s1 >> s2 >> s3 >> s4;

	int sum = s1 + s2 + s3 + s4;

	int m = sum / 60;
	int s = sum % 60;

	cout << m << '\n' << s << endl;
	return 0;
}
